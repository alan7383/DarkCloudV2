{
	"manifest_version": 3,
	"name": "DarkCloudV2 - SoundCloud Dark Mode",
	"version": "1.7.0",
	
	"description": "Changes soundcloud.com to a dark theme.",
	
	"icons": {
		"256": "icons/logo.png"
	},
	
	"content_scripts": [
		{
			"matches": ["*://*.soundcloud.com/*"],
			"exclude_globs": ["*://*.soundcloud.com/settings/connections"],
			"css": ["dark.css"]
		}
	]
}
